{
  "name": "astromahrixspace",
  "type": "module",
  "version": "1.0.0",
  "description": "Ecommerce website like amazon",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "dev:netlify": "netlify dev",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "seed:reset": "npm run data:destroy && npm run data:import",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "build:netlify": "npm install --prefix frontend && npm run build --prefix frontend",
    "db:up": "docker compose up -d mongo",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f mongo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cosmic:setup": "npm run data:destroy && npm run data:import",
    "cosmic:dev": "concurrently \"npm run dev\" \"npm start --prefix frontend\"",
    "deploy": "npm run build && echo 'Ready for deployment'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astro-mahri-creative/astromahri.space.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/astro-mahri-creative/astromahri.space/issues"
  },
  "homepage": "https://github.com/astro-mahri-creative/astromahri.space#readme",
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-plugin-react": "^7.21.2",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "immer": "^10.1.3",
    "jsonwebtoken": "^9.0.2",
    "mailgun-js": "^0.22.0",
    "mongoose": "^6.0.13",
    "multer": "^1.4.3",
    "socket.io": "^4.4.0"
  }
}
